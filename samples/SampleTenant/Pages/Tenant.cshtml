@page
@using AspNetCore.Authorization.Permissions.Abstractions
@model TenantModel

@{
    ViewData["Title"] = "Tenant";
}

<div class="text-center">
	<h1 class="display-4">Tenant</h1>
	<div class="text-start">
		<p>Tenant ID: <strong>@this.User.GetTenantId()</strong></p>
		<p>Tenant Name: <strong>@this.User.GetTenantName()</strong></p>
		<p>Tenant Display Name: <strong>@this.User.GetTenantDisplayName()</strong></p>
	</div>
</div>
